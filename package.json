{
  "name": "ostasks",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm --prefix ./backend run start:dev\" \"npm --prefix ./client run dev\"",
    "build": "npm --prefix ./backend run build && npm --prefix ./client run build",
    "start": "concurrently \"npm --prefix ./backend run start\" \"npm --prefix ./client run start\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
